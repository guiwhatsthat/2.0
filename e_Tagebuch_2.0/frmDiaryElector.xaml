<Window x:Class="e_Tagebuch_2._0.frmDiaryElector"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:e_Tagebuch_2._0"
        mc:Ignorable="d"
        Title="frmDiaryElector" Height="450" Width="800">
    <Grid>
        <Label Content="Avaiable diaries" HorizontalAlignment="Left" Margin="25,30,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="cmbDiaries" HorizontalAlignment="Left" Margin="25,61,0,0" VerticalAlignment="Top" Width="120"/>
        <Button Content="Show" HorizontalAlignment="Left" Margin="43,102,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Label Content="Create new diary" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="25,156,0,0"/>
        <Label Content="Name:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="25,187,0,0"/>
        <TextBox x:Name="txtDiaryName" HorizontalAlignment="Left" Height="24" Margin="25,218,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnCreateDiary" Content="Create diary" HorizontalAlignment="Left" Margin="25,260,0,0" VerticalAlignment="Top" Width="75" Click="btnCreateDiary_Click"/>

    </Grid>
</Window>
